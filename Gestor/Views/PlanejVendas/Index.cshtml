@model IEnumerable<Gestor.Models.PlanejVenda>

@using X.PagedList.Mvc;
@using X.PagedList;

@{
    ViewBag.Title = "Índice";
    Layout = "~/Views/Shared/_Layout.cshtml";
    IPagedList<Gestor.Models.PlanejVenda> planej = ViewBag.OnePageHistory;
}

<h2>Índice</h2>

<p>
    @Html.ActionLink("Criar novo planejamento de compra", "Create")
</p>

<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("Search", "PlanejVendas", FormMethod.Post))
        {
            <p>
                Código: @Html.TextBox("search")
                <input type="submit" value="Procurar" />
            </p>
        }
    </div>
    <div class="col-md-4">
        @using (Html.BeginForm("Incremento", "PlanejVendas", FormMethod.Post))
        {
            <p>
                Inc. Global (%): @Html.TextBox("incremento", (float)ViewBag.incremento)
                <input type="submit" value="Calcular" />
            </p>
        }
    </div>
    <div class="col-md-4">
        @using (Html.BeginForm("DespExp", "PlanejVendas", FormMethod.Post))
        {
            <p>
                Desp. Exp. (%): @Html.TextBox("DespExp", (float)ViewBag.despExp)
                <input type="submit" value="Calcular" />
            </p>
        }
    </div>
</div>

