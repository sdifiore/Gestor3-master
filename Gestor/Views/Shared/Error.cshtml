@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "Erro";
}
<h1 class="text-danger">Erro não registrável.</h1>
<h2 class="text-danger">Ocorreu um erro ao executar a sua operação.</h2>
