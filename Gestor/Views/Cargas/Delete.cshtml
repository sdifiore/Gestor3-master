@model Gestor.Models.Carga

@{
    ViewBag.Title = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Eliminar</h2>

<h3>Tem certeza que deseja eliminar o registro?</h3>
<div>
    <h4>Carga</h4>
    <hr />

    @Html.Partial("Common")

    @using (Html.BeginForm())
